{
    "name": "nswdpc/silverstripe-csp",
    "description": "SilverStripe Content Security Policy module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "csp",
        "content security policy"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "NSWDPC\\Utilities\\ContentSecurityPolicy\\": [
                "src/Controllers",
                "src/Extensions",
                "src/Jobs",
                "src/Middleware",
                "src/Models",
                "src/Services/",
                "src/Traits"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NSWDPC\\Utilities\\ContentSecurityPolicy\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-xml": "*",
        "silverstripe/cms": "^4.10.0",
        "symbiote/silverstripe-queuedjobs": "^4.9.0",
        "symbiote/silverstripe-multivaluefield": "^5.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    }
}
