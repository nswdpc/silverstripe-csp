{
    "name": "nswdpc/silverstripe-csp",
    "description": "SilverStripe Content Security Policy module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "csp",
        "content security policy"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James Ellis",
            "homepage": "https://dpc.nsw.gov.au",
            "role": "Developer"
        }
    ],
    "autoload": {
